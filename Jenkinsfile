pipeline 
{
	agent any

	stages 
	{
		stage("Checkout code from SCM") 
		{
			steps
			{
				echo "SCM  Stage"
			}
		}
		stage("Build") 
		{
			steps
			{
				echo "Build Stage"
			}
		}
		stage("Test") 
		{
			steps
			{
				echo "Test Stage"
			}
		}
		stage("Deploy") 
		{
			steps
			{
				echo "Deploy Stage"
			}
		}
	}
}
